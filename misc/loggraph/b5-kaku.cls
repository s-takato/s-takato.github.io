  \let\@oddfoot\@empty
  \let\@evenfoot\@empty
  \def\@evenhead{\if@mparswitch \hss \fi
    \hbox to \fullwidth{\autoxspacing
        {\small\thepage}\hfil\small\leftmark}}%
  \def\@oddhead{\hbox to \fullwidth{\autoxspacing
        {\small\rightmark}\hfil\small\thepage}\hss}%
  \let\@mkboth\markboth
  \def\chaptermark##1{\markboth{%
    \ifnum \c@secnumdepth >\m@ne
      \if@mainmatter
        \thechapter\hskip1zw
      \fi
    \fi
    ##1}{}}%
  \def\sectionmark##1{\markright{%
    \ifnum \c@secnumdepth >\z@ \thesection \hskip1zw\fi
    ##1}}}%
